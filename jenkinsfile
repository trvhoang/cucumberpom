pipeline {
    agent any
    stages {
        stage ('Install Stage') {

            steps {
                withMaven(maven : 'apache-maven-3.6.3') {
                    bat 'mvn clean install'
                }
            }
        }
        stage ('Install Stage') {
                    steps {
                        withMaven(maven : 'apache-maven-3.6.3') {
                            bat 'mvn clean install'
                        }
                    }
                }
        stage ('Testing Stage') {

            steps {
                withMaven(maven : 'apache-maven-3.6.3') {
                    bat 'mvn -Dtest=SignUpAPI test'
                }
            }
        }

    }
}